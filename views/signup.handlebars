
<div class="split login-left"><div

<div class="card">
  <h3 class="card-header">Sign Up</h3>
  <form id="signup-form" class="card-body">
    <label for="username-signup" class="form-label" id="sign-up">Username</label>
    <input id="username-signup" type="text" class="form-input" />


    <label class="form-label" for="password-signup" id="sign-up">Password</label>
    <input id="password-signup" type="password" class="form-input" />

    <button type="submit" class="btn">Signup</button>
    <a class="login-link" href="/login" id="sign-up">Back to Login</a>
  </form>
</div>
</div>

<div class="split login-right"><div>

<script src="/javascript/signup.js"></script>